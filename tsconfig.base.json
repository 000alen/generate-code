{
  "compilerOptions": {
    "disableSizeLimit": true,
    "declaration": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "downlevelIteration": true,
    "inlineSourceMap": true,
    "experimentalDecorators": true,
    "jsx": "react-jsx"
  }
}
